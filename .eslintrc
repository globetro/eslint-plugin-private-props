{
  'parser': 'babel-eslint',
  
  'env': {
    'jasmine': true,
    'node': true,
    'browser': true,
    'builtin': true
  },

  'rules': {
    'indent': [2, 2, {'SwitchCase': 1}],
    'block-scoped-var': 0,
    'camelcase': 0,
    'curly': [
      2,
      'all'
    ],
    'eqeqeq': [
      2
    ],
    'global-strict': [
      0,
      'never'
    ],

    'comma-dangle': [2, 'never'],
    'no-extra-semi': 2,
    'guard-for-in': 2,
    'new-cap': 2,
    'no-bitwise': 2,
    'no-dupe-keys': 2,
    'no-caller': 2,
    'no-empty': 2,
    'no-extend-native': 2,
    'no-irregular-whitespace': 2,
    'no-iterator': 0,
    'no-new': 0,
    'no-redeclare': 0,
    'no-sequences': 2,
    'no-undef': 2,
    'no-unused-vars': 2,
    'no-use-before-define': [2, 'nofunc'],
    'no-with': 2,
    'jsx-quotes': [2, 'prefer-single'],
    'quotes': [
      2,
      'single',
      'avoid-escape'
    ],
    'semi': [2, 'always'],
    'valid-typeof': 0,
    'wrap-iife': [
      0,
      'inside'
    ]
  },
  'globals': {
    'ENV': true,
    'CONFIG': true,
    'VERSION': true,
    'require': true,
    'process': true,
    '_': true,
    'React': true,
    'cssModule': true,
    '__': true,
    'expect': true,
    'jlet': true,
    'Promise': true
  }
}